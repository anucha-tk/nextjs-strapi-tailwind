{
  "name": "nextjs",
  "version": "2.1.0",
  "private": true,
  "scripts": {
    "start": "next dev",
    "dev": "dotenv -e ./.env.development cross-env INIT_CWD=$PWD next dev",
    "build": "cross-env INIT_CWD=$PWD next build && cross-env NODE_OPTIONS='--experimental-json-modules' node ./scripts/postbuild.mjs",
