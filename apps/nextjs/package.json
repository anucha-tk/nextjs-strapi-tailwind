    "@tanstack/react-query": "^5.17.19",
    "autoprefixer": "^10.4.13",
    "dotenv-cli": "^7.3.0",
    "esbuild": "0.18.11",
    "github-slugger": "^2.0.0",
    "gray-matter": "^4.0.2",
    "image-size": "1.0.0",
    "moment": "^2.30.1",
    "next": "14.0.3",
    "next-themes": "^0.2.1",
    "postcss": "^8.4.24",
    "react": "18.2.0",
